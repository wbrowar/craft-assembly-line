{
    "require": {
        "craftcms/cms": "^3.6.0-RC1",
        "vlucas/phpdotenv": "^3.4.0",
        "wbrowar/guide": "@dev"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/"
